{
  "manifestVersion": 1,
  "id": "bitbucket-helpers",
  "name": "BitBucket Helper Tasks",
  "version": "0.1.0",
  "publisher": "mikezuidhoek",
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Provides tasks for communicating between VSTS and BitBucket",
  "categories": [
    "Build and release"
  ],
  "icons": {
    "default": "key-128.png"
  },
  "tags": ["BitBucket", "CI", "Continuous", "Integration", "Publish", "Build", "Result"],
  "screenshots": [{
    "path": "screenshots/BitBucketBuildResult.png"
  }],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "license.md"
    }
  },
  "files": [
    {
      "path": "Tasks/BitBucketBuildResult"
    }
  ],
  "contributions": [
    {
      "id": "bitbucket-buildresult",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/BitBucketBuildResult"
      }
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mzuidhoek/BitBucket-VSTS-Tasks"
  }
}
